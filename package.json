{
  "name": "lmksystems-select-input",
  "version": "1.0.0",
  "description": "A React Native SelectInput for iOS and Android, which uses a keyboard with a Picker component",
  "author": "LMK systeme based on Markus Wind work",
  "license": "MIT",
  "homepage": "https://github.com/michael-lmk/react-native-select-input-ios.git",
  "repository": {
    "type": "git",
    "url": "git@github.com:michael-lmk/react-native-select-input-ios.git"
  },
  "keywords": [
    "ReactNative",
    "Select",
    "SelectInput",
    "SelectInputIOS",
    "SelectInputAndroid",
    "Picker",
    "PickerIOS",
    "PickerAndroid",
    "CustomKeyboard"
  ],
  "main": "./src/index.js",
  "scripts": {
    "lint": "./node_modules/.bin/eslint **/*.js",
    "test": "./node_modules/.bin/jest"
  },
  "jest": {
    "preset": "react-native",
    "automock": false,
    "testMatch": [
      "<rootDir>/src/**/__tests__/*.spec.js"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "<rootDir>/node_modules",
      "<rootDir>/src"
    ]
  },
  "devDependencies": {
    "babel-eslint": "*",
    "babel-preset-react-native": "*",
    "eslint": "*",
    "eslint-config-prettier": "*",
    "eslint-plugin-prettier": "*",
    "eslint-plugin-react": "*",
    "jest": "*",
    "prettier": "*",
    "react": "16.5.0",
    "react-native": "^0.66.0",
    "react-test-renderer": "*"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
